{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "tsconfig": {
        "allowJs": true
      }
    }]
  },
  "moduleNameMapper": {
    "^@webchat/common(.*)$": "<rootDir>/../../common/src$1"
  }
}
